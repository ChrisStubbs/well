@model PH.Well.Dashboard.Models.BootstrapData
@{
    ViewBag.Title = "User Preferences";
}
@section scripts{
    <script>
        var serverConfig = @Html.Raw(Model.Configuration);
        System.import('app/user_preferences/main')
            .then(function(m) { m.runApplication(serverConfig); })
            .catch(console.error.bind(console));
    </script>
}
<ow-user-preferences>Loading User preferences...</ow-user-preferences>
