                    <xs:element name="RouteDate" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="DriverName" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="VehicleReg" type="xs:string" minOccurs="1" maxOccurs="1"/>
