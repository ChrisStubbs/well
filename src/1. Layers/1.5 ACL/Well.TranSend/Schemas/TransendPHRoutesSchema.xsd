                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Stops" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="1" maxOccurs="unbounded" name="Stop">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="PlannedStopNumber" type="xs:unsignedByte" minOccurs="1" maxOccurs="1"/>
