                                      <xs:element name="Longitude" type="xs:decimal" minOccurs="0"/>
                                      <xs:element name="IsDropAndDrive" minOccurs="0"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element minOccurs="0" maxOccurs="unbounded" name="EntityAttributes">
                                  <xs:complexType>
