                                                        <xs:element name="OrderedQty" type="xs:unsignedByte" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="SkuWeight" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="SkuCube" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="UnitMeasure" type="xs:unsignedByte" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="TextField1" type="xs:unsignedByte" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="TextField2" type="xs:unsignedByte" minOccurs="1" maxOccurs="1"/>
                                                        <xs:element name="TextField3" minOccurs="0" maxOccurs="1"/>
                                                        <xs:element name="TextField4" minOccurs="0" maxOccurs="1"/>
                                                        <xs:element name="TextField5" minOccurs="0" maxOccurs="1"/>
