                                            <xs:element name="JobRef1" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
                                            <xs:element name="JobRef2" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
                                            <xs:element name="JobRef3" minOccurs="0" maxOccurs="1"/>
